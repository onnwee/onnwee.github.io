{
  "mcpServers": {
    "upstash_context7": {
      "type": "http",
      "url": "https://mcp.context7.com/mcp",
      "headers": {
        "Authorization": "${context7_api_key}"
      },
      "tools": []
    },
    "github_github-mcp-server": {
      "type": "http",
      "url": "https://api.githubcopilot.com/mcp/",
      "tools": []
    },
    "microsoft_playwright-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ],
      "tools": []
    },
    "microsoftdocs_mcp": {
      "type": "http",
      "url": "https://learn.microsoft.com/api/mcp",
      "tools": []
    },
    "evalstate_hf-mcp-server": {
      "type": "http",
      "url": "https://huggingface.co/mcp?login",
      "tools": []
    },
    "cognitionai_deepwiki": {
      "type": "http",
      "url": "https://mcp.deepwiki.com/sse",
      "tools": []
    },
    "stripe_agent-toolkit": {
      "type": "http",
      "url": "https://mcp.stripe.com",
      "tools": []
    },
    "oraios_serena": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "serena==latest",
        "--context",
        "ide-assistant"
      ],
      "tools": []
    },
    "apify_apify-mcp-server": {
      "type": "http",
      "url": "https://mcp.apify.com",
      "tools": []
    },
    "chroma-core_chroma-mcp": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "chroma-mcp==v0.2.6"
      ],
      "env": {
        "CHROMA_CLIENT_TYPE": "${input:chroma_client_type}",
        "CHROMA_DOTENV_PATH": "${input:chroma_dotenv_path}",
        "CHROMA_DATA_DIR": "${input:chroma_data_dir}",
        "CHROMA_TENANT": "${input:chroma_tenant}",
        "CHROMA_DATABASE": "${input:chroma_database}",
        "CHROMA_API_KEY": "${input:chroma_api_key}",
        "CHROMA_HOST": "${input:chroma_host}",
        "CHROMA_PORT": "${input:chroma_port}",
        "CHROMA_CUSTOM_AUTH_CREDENTIALS": "${input:chroma_custom_auth}",
        "CHROMA_SSL": "${input:chroma_ssl}"
      },
      "tools": []
    },
    "codacy_codacy-mcp-server": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@codacy/codacy-mcp@latest"
      ],
      "env": {
        "CODACY_ACCOUNT_TOKEN": "${input:codacy_account_token}",
        "CODACY_CLI_VERSION": "${input:codacy_cli_version}"
      },
      "tools": []
    },
    "chromedevtools_chrome-devtools-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "chrome-devtools-mcp@latest",
        "--browserUrl",
        "${input:browser_url}",
        "--headless",
        "${input:headless}",
        "--isolated",
        "${input:isolated}",
        "--channel",
        "${input:chrome_channel}"
      ],
      "tools": []
    }
  }
}
